          },
          {
            "group": "Supporting",
            "openapi": {
              "source": "specifications/split/supporting_openapi.json",
              "directory": "api-reference"
            }
          }
        ]
      },
      {
        "tab": "Subscription Channels",
        "asyncapi": "specifications/deribit_asyncapi.json"
      },
      {
        "tab": "FIX API",
        "groups": [
          {
            "group": "FIX API Documentation",
