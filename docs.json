{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Mint Starter Kit",
  "colors": {
    "primary": "#0052FF",
    "light": "#FFFFFF",
    "dark": "#0A0B0D"
  },
  "customCss": [
    "/public/style.css"
  ],
  "headTags": [
    {
      "tag": "script",
      "attributes": {
        "src": "/public/replace-query-params.js",
        "type": "text/javascript"
      }
    }
  ],
  "api": {
    "playground": {
      "display": "none"
    }
  },
  "navigation": {
    "tabs": [
      {
        "tab": "API Overview",
        "groups": [
          {
            "group": "Introduction",
            "expanded": false,
            "pages": [
              "mdfx_files/welcome",
              "mdfx_files/deribit-quickstart"
            ]
          },
          {
            "group": "Authentication",
            "pages": [
              "mdfx_files/api-keys",
              "mdfx_files/oauth"
            ]
          },
          {
            "group": "JSON-RPC Protocol",
            "pages": [
              "mdfx_files/overview",
              "mdfx_files/request-format",
              "mdfx_files/response-format",
              "mdfx_files/errors"
            ]
          }
        ]
      },
      {
        "tab": "Methods",
        "groups": [
          {
            "group": "Authentication",
            "openapi": {
              "source": "specifications/split/authentication_openapi.json",
              "directory": "api-reference"
            }
          },
          {
            "group": "Trading",
